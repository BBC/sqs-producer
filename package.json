{
  "name": "sqs-producer",
  "version": "1.6.3",
  "description": "Enqueues messages onto a given SQS queue",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "coverage": "nyc mocha && nyc report --reporter=html && nyc report --reporter=json-summary",
    "lint": "eslint '**/**.js'"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bbc/sqs-producer.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bbc/sqs-producer/issues"
  },
  "keywords": [
    "aws",
    "sqs",
    "producer",
    "queue"
  ],
  "homepage": "https://github.com/bbc/sqs-producer",
  "dependencies": {
    "aws-sdk": "^2.393.0"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.5.1",
    "eslint": "^6.8.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "sinon": "^7.2.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended"
  },
  "eslintIgnore": [
    "node_modules",
    "lib-cov",
    ".nyc_output",
    "coverage"
  ]
